{
  "version": 2,
  "builds": [
    {
      "src": "client/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/pages/$1"
    },
    {
      "src": "/",
      "dest": "/client/pages/index.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/client/assets/$1"
    }
  ]
} 